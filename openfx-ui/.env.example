# Example environment file for development.
# Copy this to `.env` and replace placeholder values with real secrets locally.

# Server port
PORT=4000

# Frontend dev server -> API URL (Vite)
VITE_API_URL=http://localhost:4000

# MongoDB connection (local example)
MONGO_URI=mongodb://localhost:27017/openfx

# CORS origin allowed for frontend
CORS_ORIGIN=http://localhost:3000

# API keys (DO NOT commit real keys)
FINNHUB_API_KEY=your_finnhub_api_key_here
OPENEXCHANGE_API_KEY=your_openexchange_api_key_here
